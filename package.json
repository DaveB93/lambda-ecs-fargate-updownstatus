{
  "name": "lambda-ecs-fargate-updownstatus",
  "version": "1.0.0",
  "description": "A set of lambda functions using AWS SDK for JavaScript V3 to adjust the desired capacity and see the status of an ECS Fargate cluster ( for Valheim initially )",
  "main": "index.js",
  "repository": "",
  "author": "dave_b93",
  "license": "MIT",
  "scripts": {
    "build:backend": "webpack build ./src/startstopserver.ts --mode development --target node --devtool eval -o ./src/startstop",
    "build:backendstatus": "webpack build ./src/serverstatus.ts --mode development --target node --devtool eval -o ./src/serverstatus",
    "test": "exit 0"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^1.0.0-gamma.1",
    "@aws-sdk/client-ec2": "^3.8.0",
    "@aws-sdk/client-ecs": "^3.8.0",
    "@aws-sdk/client-iam": "^1.0.0-gamma.2",
    "@aws-sdk/client-lambda": "^1.0.0-gamma.2",
    "@aws-sdk/client-s3": "^1.0.0-gamma.1",
    "@aws-sdk/credential-provider-cognito-identity": "^1.0.0-gamma.2"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "typescript": "^4.0.2",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0"
  }
}
